{
  "scripts": {
    "start": "node dist/app.js",
    "build2": "tsc",
    "dev2": "nodemon --exec ts-node src/index.ts",
    "dev": "tsnd --respawn -r tsconfig-paths/register --pretty --transpile-only ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start:dist": "node dist/src/index.js",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mongoose": "^8.7.3",
    "neo4j-driver": "^5.26.0",
    "node": "^23.1.0",
    "prisma": "^5.21.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.0",
    "tsnd": "^1.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.0",
    "@types/pg": "^8.11.10",
    "install": "^0.13.0",
    "nodemon": "^3.1.7",
    "npm": "^10.9.0",
    "pg": "^8.13.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
